# for scripts
Preprocessor=@OMNI_HOME@/bin/omcpp
L2X=@OMNI_HOME@/bin/omc2x
LX2X=@OMNI_HOME@/bin/omcx2x
X2L=@OMNI_HOME@/bin/omx2c
NativeCompiler=@OMNI_HOME@/bin/omcnative
Linker=@OMNI_HOME@/bin/omclinker

# for options
PreprocessorOption=@GASNET_CPPFLAGS@ @GASNET_CFLAGS@
L2XOption=
LX2XOption=@OMC_CONF_LX2X_OPT@
X2LOption=
NativeCompilerOption=@OMC_CONF_NTV_OPT@ @GASNET_CPPFLAGS@ @GASNET_CFLAGS@
LinkerOption=@GASNET_LDFLAGS@ @GASNET_LIBS@ @OMC_CONF_LNK_OPT@
TempDir=/tmp
